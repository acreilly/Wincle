<%= form_for @employer do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.telephone_field(:user, :phone) %>
  <%= f.email_field(:user, :address) %>
  <%= f.label :industry %>
  <%= f.text_field :industry  %>
  <%= f.label :company %>
  <%= f.text_field :company  %>
  <%= f.submit "Save Employer Profile" %>
<% end %>
